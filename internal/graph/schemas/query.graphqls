scalar Date

type Merchant {
  id: ID!
  name: String!
  category: String!
  location: String!
  latitude: Float!
  longitude: Float!
  createdAt: String!
  verifiedAt: String
  cryptoChains: [CryptoChain!]!
}

type CryptoChain {
  id: ID!
  name: String!
  symbol: String!
  imagePath: String!
  createdAt: String!
}

type Query {
  ping: String!
  getMerchants: [Merchant!]!
  getMerchantByID(id: ID!): Merchant
}